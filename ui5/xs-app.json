{
	"welcomeFile": "/ui5/webapp/index.html",
	"authenticationMethod": "route",
	"routes": [{
		"source": "(.*)(.xsjs)",
		"destination": "core-xsjs-backend",
		"csrfProtection": false,
		"authenticationType": "xsuaa"
	},{
		"source": "(.*)(.xsodata)",
		"destination": "core-xsjs-backend",
		"csrfProtection": false,
		"authenticationType": "xsuaa"
	},{
		"source": "node(.*)",
		"destination": "core-nodejs-backend",
		"csrfProtection": true,
		"authenticationType": "xsuaa"
	},{
		"source": "/(.*)",
		"localDir": "webapp",
		"authenticationType": "xsuaa"
	}]
}