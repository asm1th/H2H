{
	"welcomeFile": "/ui5/index.html",
	"authenticationMethod": "none",
	"routes": [{
 		"source": "/node(.*)",
 		"destination": "core-xsjs-backend",
 		"csrfProtection": false
 	},{
		"source": "(.*)(.xsjs)",
		"csrfProtection": false,
		"destination": "core-xsjs-backend"
	},{
		"source": "(.*)(.js)",
		"csrfProtection": false,
		"destination": "core-nodejs-backend"
	},{
		"source": "(.*)(.xsodata)",
		"destination": "core-xsjs-backend"
	}]
}