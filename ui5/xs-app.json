{
	"welcomeFile": "/index.html",
	"authenticationMethod": "route",
	"logout": {
		"logoutEndpoint": "/do/logout"
     },
	"routes": [{
		"source": "(.*)(.xsjs)",
		"csrfProtection": true,
		"destination": "core-xsjs-backend"
	},{
		"source": "(.*)(.xsodata)",
		"csrfProtection": true,
		"destination": "core-xsjs-backend"
	},{
		"source": "node(.*)",
		"csrfProtection": true,
		"destination": "core-nodejs-backend"
	},{
		"source": "^(.*)$",
		"target": "$1",
		"service": "html5-apps-repo-rt",
		"authenticationType": "xsuaa"  
	}]
}