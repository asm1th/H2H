{
	"welcomeFile": "/webapp/index.html",
	"authenticationMethod": "none",
	"logout": {
		"logoutEndpoint": "/logout"
     },
	"routes": [{
		"source": "(.*)(.xsjs)",
		"csrfProtection": true,
		"destination": "core-xsjs-backend"
	},{
		"source": "(.*)(.xsodata)",
		"csrfProtection": true,
		"destination": "core-xsjs-backend"
	},{
		"source": "node(.*)",
		"csrfProtection": true,
		"destination": "core-nodejs-backend"
	},{
		"source": "^/ui5/(.*)$",
		"target": "$1",
		"localDir": "webapp",
		"authenticationType": "xsuaa"  
	}]
}