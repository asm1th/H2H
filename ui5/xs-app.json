{
	"welcomeFile": "/webapp/index.html",
	"authenticationMethod": "route",
	"routes": [{
		"source": "(.*)(.xsjs)",
		"csrfProtection": false,
		"destination": "core-xsjs-backend"
	},{
		"source": "(.*)(.xsodata)",
		"csrfProtection": false,
		"destination": "core-xsjs-backend"
	},{
		"source": "node(.*)",
		"destination": "core-nodejs-backend",
		
		"csrfProtection": true,
		"authenticationType": "xsuaa"
	}]
}